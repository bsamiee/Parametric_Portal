    "engines": {
        "node": "25.2.1"
    },
    "name": "parametric-portal",
    "packageManager": "pnpm@10.26.1",
    "pnpm": {
        "overrides": {
            "jiti": "2.6.1",
            "node-forge": "1.3.3",
            "puppeteer": "24.34.0"
        },
        "peerDependencyRules": {
            "allowedVersions": {
                "puppeteer": "24.34.0",
                "typescript": "6.0.0-dev.20251125",
                "vitest": "4.0.16"
            }
        }
    },
    "private": true,
    "scripts": {
        "affected": "nx affected -t build,test,typecheck,check",
        "build:all": "nx run-many -t build",
        "build:libs": "nx run-many -t build --projects=@parametric-portal/types,@parametric-portal/theme,@parametric-portal/runtime,@parametric-portal/components,@parametric-portal/devtools",
        "check:all": "nx run-many -t check",
        "clean": "rm -rf .nx/cache node_modules/.cache packages/*/dist apps/*/dist packages/*/.vite-inspect apps/*/.vite-inspect",
        "dev:icons": "nx dev @parametric-portal/parametric-icons",
        "fix:all": "nx run-many -t fix",
        "graph": "nx graph",
        "prepare": "lefthook install",
        "quality": "pnpm exec nx run-many -t typecheck,check --parallel=4",
        "quality:affected": "pnpm exec nx affected -t typecheck,check --base=origin/main",
        "reset": "pnpm clean && pnpm install && pnpm build:libs",
        "sonar": "npx @sonar/scan",
        "test:all": "nx run-many -t test",
        "tokens": "tsx tools/scripts/count-tokens.ts",
        "typecheck:all": "nx run-many -t typecheck"
    },
    "type": "module",
    "version": "0.4.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
