{
  "name": "@parametric-portal/server",
  "version": "1.0.6",
  "type": "module",
  "exports": {
    "./api": {
      "types": "./src/api.ts",
      "import": "./src/api.ts",
      "default": "./src/api.ts"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "import": "./src/errors.ts",
      "default": "./src/errors.ts"
    },
    "./middleware": {
      "types": "./src/middleware.ts",
      "import": "./src/middleware.ts",
      "default": "./src/middleware.ts"
    },
    "./crypto": {
      "types": "./src/crypto.ts",
      "import": "./src/crypto.ts",
      "default": "./src/crypto.ts"
    },
    "./telemetry": {
      "types": "./src/telemetry.ts",
      "import": "./src/telemetry.ts",
      "default": "./src/telemetry.ts"
    },
    "./metrics": {
      "types": "./src/metrics.ts",
      "import": "./src/metrics.ts",
      "default": "./src/metrics.ts"
