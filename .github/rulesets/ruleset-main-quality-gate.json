{
    "bypass_actors": [
        {
            "actor_id": 5,
            "actor_type": "RepositoryRole",
            "bypass_mode": "always"
        }
    ],
    "conditions": {
        "ref_name": {
            "exclude": [],
            "include": ["refs/heads/main"]
        }
    },
    "enforcement": "active",
    "name": "00-main-quality-gate",
    "rules": [
        {
            "parameters": {
                "required_status_checks": [
                    {
                        "context": "CI / Code Quality",
                        "integration_id": null
                    },
                    {
                        "context": "Security Scanning / Dependency Audit",
                        "integration_id": null
                    },
                    {
                        "context": "Security Scanning / CodeQL Analysis",
                        "integration_id": null
                    },
                    {
                        "context": "Security Scanning / Secrets Scan",
                        "integration_id": null
                    },
                    {
                        "context": "Security Scanning / License Compliance",
                        "integration_id": null
                    }
                ],
                "strict_required_status_checks_policy": true
            },
            "type": "required_status_checks"
        },
        {
            "type": "required_linear_history"
        },
        {
            "parameters": {
                "dismiss_stale_reviews_on_push": true,
                "require_code_owner_review": false,
                "require_last_push_approval": false,
                "required_approving_review_count": 0,
                "required_review_thread_resolution": true
            },
            "type": "pull_request"
        }
    ],
    "target": "branch"
}
